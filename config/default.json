{
   "db": {
      "db_type": "${DB_TYPE}",
      "driver": "${DB_DRIVER}", 
      "dbname": "${DB_NAME}",
      "host": "${DB_HOST}",
      "port": "${DB_PORT}",
      "user": "${DB_USER}",
      "password": "${DB_PASSWORD}",
      "table": "SETTLEPLATE"
   },
   "regex": {
      "user": [
         "^user:(?P<user>.+)$"
      ],
      "batch": [
         "^(?P<batch>[A-Za-z]{3,5}\\d{7})$",
         "^(?P<batch>[A-Za-z]{3,5}\\d{8}-\\d)$",
         "^(?P<batch>MK.*\\d{7})$"
      ],
      "location": [
         "^loc:(?P<location>.+)$"
      ],
      "settleplate": [
         "^(?P<serial>\\d{18}(?P<year>\\d{2})(?P<month>\\d{2})(?P<day>\\d{2})\\d{2}(?P<lot>\\d{10})\\d{8})$",
         "^(?P<serial>\\d+(?P<lot>\\d{11})(?P<year>\\d{2})(?P<month>\\d{2})(?P<day>\\d{2}))$",
         "^(?P<serial>\\d+\\s?(?P<lot>\\d{10})(?P<year>\\d{4})(?P<month>\\d{2})(?P<day>\\d{2})\\w{9})$"
      ]
   },
   "general": {
      "adminpwd": "admin",
      "user_min": 4,
      "user_max": 8,
      "timeout": 3600,
      "grace_period": 3600,
      "mountpoint": "/mnt/data",
      "savepath": "/mnt/data/Data/Colonizer/",
      "autocount": true,
      "sameuser": true,
      "testserver": false,
      "positive_test_required": true,
      "positive_test_location": "PositiveTest"
   },
   "users": {
      "admin": "admin"
   },
   "hive": {
      "confidence_threshold_low": 0.2,
      "confidence_threshold_high": 0.5,
      "iou_threshold": 0.5
   },
   "camera": {
      "_default": {
         "cam_crop": [676, 30, 3008, 3008],
         "cam_hflip": false,
         "cam_vflip": true,
         "cam_rotation": "cw",
         "cam_exposure": 50000,
         "cam_wb": [2.4,2.4],
         "led_top": [0,0,0],
         "led_ring": [0,0,0],
         "led_status": [0,0,0],
         "led_wait": 0.7,
         "crop_auto" : "none",
         "crop_mask" : false,
         "crop_mask_cx": 0.506,
         "crop_mask_cy": 0.498,
         "crop_mask_cr": 0.434,
         "crop_drawonly" : false,
         "crop_canny_t1": 32,
         "crop_canny_t2": 64,
         "histogram" : false,
         "autolevel": false
      },
      "settleplate": {
         "cam_exposure": 200000,
         "led_ring": [255,128,32],
         "crop_mask": true,
         "crop_mask_cr": 0.434
      },
      "plate": {
         "cam_exposure": 50000,
         "led_top": [255,112,17],
         "crop_auto": "rect",
         "crop_mask_cr": 0.54,
         "crop_canny_t1": 64,
         "crop_canny_t2": 128,
         "default": true
      }
   }
}